  if version < 508
    let did_sgmllnx_syn_inits = 1
    command -nargs=+ HiLink hi link <args>
  else
    command -nargs=+ HiLink hi def link <args>
  endif

  HiLink sgmllnxTag2	    Function
  HiLink sgmllnxTagN2	    Function
