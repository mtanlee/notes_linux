/* For the convenience of QuickTime */
enum {
  srcCopy                       = 0
};

/* needed in ImageCompression.h */
/* Needed in HIServices/Icons.i */
struct BitMap {
  Ptr                 baseAddr;
  short               rowBytes;
  Rect                bounds;
};
