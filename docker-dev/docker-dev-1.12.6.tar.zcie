 *     4 byte length of policy data,
 *     policy data
 *
 */
#define		kDSStdAuthSetPolicy						"dsAuthMethodStandard:dsAuthSetPolicy"

/*!
 * @defined kDSStdAuthSetPolicyAsRoot
