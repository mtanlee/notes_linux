func TestInitialPermute(t *testing.T) {
	for i := uint(0); i < 64; i++ {
		bit := uint64(1) << i
		got := permuteInitialBlock(bit)
		want := uint64(1) << finalPermutation[63-i]
		if got != want {
			t.Errorf("permute(%x) = %x, want %x", bit, got, want)
		}
	}
}

