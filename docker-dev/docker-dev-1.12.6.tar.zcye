    let did_change_syntax_inits = 1
    command -nargs=+ HiLink hi link <args>
  else
    command -nargs=+ HiLink hi def link <args>
  endif

  HiLink changeFromMaterial String
  HiLink changeToMaterial Statement

  delcommand HiLink
endif

