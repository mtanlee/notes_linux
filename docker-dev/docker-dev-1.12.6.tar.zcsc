.Os
.Sh NAME
.Nm pthread_attr_destroy ,
.Nm pthread_attr_getdetachstate ,
.Nm pthread_attr_getinheritsched ,
.Nm pthread_attr_getschedparam ,
.Nm pthread_attr_getschedpolicy ,
