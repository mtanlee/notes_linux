HIMouseTrackingGetParameters(
  OSType       inSelector,
  EventTime *  outTime,           /* can be NULL */
  HISize *     outDistance)       /* can be NULL */           AVAILABLE_MAC_OS_X_VERSION_10_3_AND_LATER;


