  const AppleEvent *  theAppleEvent,
  const AppleEvent *  reply,
  AEEventHandlerUPP   dispatcher,          /* can be NULL */
  SRefCon             handlerRefcon)                          AVAILABLE_MAC_OS_X_VERSION_10_0_AND_LATER;


