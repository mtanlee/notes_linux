CSSM_RETURN CSSMAPI
MDS_Terminate (MDS_HANDLE MdsHandle)
	DEPRECATED_IN_MAC_OS_X_VERSION_10_7_AND_LATER;

CSSM_RETURN CSSMAPI
MDS_Install (MDS_HANDLE MdsHandle)
	DEPRECATED_IN_MAC_OS_X_VERSION_10_7_AND_LATER;

CSSM_RETURN CSSMAPI
MDS_Uninstall (MDS_HANDLE MdsHandle)
	DEPRECATED_IN_MAC_OS_X_VERSION_10_7_AND_LATER;

#ifdef __cplusplus
}
#endif

