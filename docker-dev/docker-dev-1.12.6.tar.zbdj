        },
        "Schema":{
          "shape":"DataSchema",
          "documentation":"<p>The schema used by all of the data files referenced by the <code>DataSource</code>.</p> <note><title>Note</title> <p>This parameter is provided as part of the verbose format.</p></note>"
        }
