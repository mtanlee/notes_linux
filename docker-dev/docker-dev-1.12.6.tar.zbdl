    "ChapInfo":{
      "type":"structure",
      "members":{
        "TargetARN":{
          "shape":"TargetARN",
          "documentation":"<p>The Amazon Resource Name (ARN) of the volume.</p> <p><i>Valid Values</i>: 50 to 500 lowercase letters, numbers, periods (.), and hyphens (-).</p>"
        },
        "SecretToAuthenticateInitiator":{
          "shape":"ChapSecret",
