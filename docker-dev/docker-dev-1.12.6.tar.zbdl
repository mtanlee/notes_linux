        "StatusCode":{
          "shape":"BounceStatusCode",
          "documentation":"<p>The SMTP enhanced status code, as defined by <a href=\"https://tools.ietf.org/html/rfc3463\">RFC 3463</a>.</p>"
        },
        "Message":{
